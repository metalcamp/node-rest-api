{
  "name": "express-rest-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "compile": "npx tsc",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "npx tsc",
    "build-start": "\"yarn run build\" && \"yarn run start\"",
    "start": "env START_SERVER=true node dist/index.js",
    "start-rss": "env START_RSS_WORKER=true node dist/index.js",
    "start-publish": "env START_PUBLISH_WORKER=true node dist/index.js",
    "start-redis": "env START_REDIS_LISTENER_WORKER=true node dist/index.js",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"yarn run watch-ts\" \"yarn run watch-node\"",
    "test": "jest"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/hapi__joi": "^16.0.12",
    "@types/ioredis": "^4.16.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "@types/xml2js": "^0.4.5",
    "concurrently": "^5.1.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "ts-jest": "^25.5.0",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-winston": "^4.0.3",
    "fs": "^0.0.1-security",
    "ioredis": "^4.16.3",
    "multimatch": "^4.0.0",
    "pg": "^8.0.2",
    "reflect-metadata": "^0.1.13",
    "rss-parser": "^3.7.6",
    "typeorm": "^0.2.24",
    "winston": "^3.2.1"
  }
}
